{
  "name": "rtk-map",
  "displayName": "RTK Map Hook Navigator",
  "description": "Navigate from RTK Query auto-generated hooks to their endpoint definitions with CodeLens and Go to Definition support",
  "version": "0.0.1",
  "publisher": "enBonnet",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "keywords": [
    "rtk",
    "redux",
    "query",
    "typescript",
    "navigation",
    "hooks",
    "codelens",
    "definition"
  ],
  "icon": "assets/rtk-map.png",
  "galleryBanner": {
    "color": "#007acc",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/enBonnet/rtk-map"
  },
  "homepage": "https://github.com/enBonnet/rtk-map",
  "bugs": {
    "url": "https://github.com/enBonnet/rtk-map/issues"
  },
  "activationEvents": [
    "onLanguage:typescript"
  ],
  "main": "./dist/extension.js",
  "contributes": {},
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "compile": "pnpm run check-types && pnpm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "pnpm run check-types && pnpm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "format": "prettier --write .",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.90.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.27.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3"
  }
}
